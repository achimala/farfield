<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.farfield.tunnel</string>

    <key>ProgramArguments</key>
    <array>
      <string>__HOME__/bin/run-farfield-tunnel.sh</string>
    </array>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <dict>
      <key>SuccessfulExit</key>
      <false/>
      <key>NetworkState</key>
      <true/>
    </dict>

    <key>StandardOutPath</key>
    <string>__HOME__/Library/Logs/farfield/tunnel.out.log</string>

    <key>StandardErrorPath</key>
    <string>__HOME__/Library/Logs/farfield/tunnel.err.log</string>

    <key>ProcessType</key>
    <string>Background</string>

    <key>ThrottleInterval</key>
    <integer>10</integer>
  </dict>
</plist>
